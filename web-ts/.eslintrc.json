{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "warn",
    "comma-style": "warn",
    "curly": "warn",
    "block-scoped-var": "warn",
    "arrow-body-style": "warn",
    "constructor-super": "warn",
    "brace-style": "warn",
    "arrow-spacing": "warn",
    "complexity": "warn",
    "default-case": "off",
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "valid-typeof": "warn"
  }
}
